{
    "name": "Jenkins",
    "id": "jenkins",
    "description": {
        "en": "An extendable open source continuous integration server",
        "fr": "Un serveur d'integration continue open-source"
    },
    "developer": {
        "name": "",
        "email": "",
        "url": "http://jenkins-ci.org/0"
    },
    "multi_instance": "false",
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "ask": {
                    "en": "Choose a domain for Jenkins"
                },
                "example": "domain.org"
            },
            {
                "name": "path",
                "ask": {
                    "en": "Choose a path for Jenkins"
                },
                "example": "/jenkins",
                "default": "/jenkins"
            },
            {
                "name": "admin",
                "ask": {
                    "en": "Choose the Jenkins administrator (must be an existing YunoHost user)"
                },
                "example": "homer"
            }
        ]
    }
}

